<%- if @community.is_institution? -%>
$("#connection_row_<%= @person.id %>").html("<%= escape_javascript(render(:partial => 'institution_connections_table_row', :locals => { community: @community, person: @person })) %>")
<%- else -%>
$("#connection_row_<%= @person.id %>").html("<%= escape_javascript(render(:partial => 'community_connections_table_row', :locals => { community: @community, person: @person })) %>")
<%- end -%>